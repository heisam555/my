int contatorebuy = 0;
double Prezzosell = 0;
int contatoresell = 0;
for(int i = OrdersTotal()-1; i >= 0; i--)
{
if(OrderSelect(i,SELECT_BY_POS,MODE_TRADES))
{
if (OrderMagicNumber() == MagicNumber && OrderSymbol() == _Symbol )
   {
   int tipo = OrderType();
      if ( tipo == OP_BUY)
      {
      Prezzobuy += OrderOpenPrice();
      contatorebuy++;
      }
      else if ( tipo == OP_SELL)
      {
      Prezzosell += OrderOpenPrice();
      contatoresell++;
      }
      
   }
}
