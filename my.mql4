bool Hour_trade_allowed(int CoefPartenza, int CoefFine)
{

int CoefOrario = (Hour()*60)+Minute();
   bool trade = false;
   if(CoefPartenza > CoefFine){
     if (CoefOrario >= CoefPartenza || CoefOrario < CoefFine) trade = true;
   } else
     if (CoefOrario >= CoefPartenza && CoefOrario < CoefFine) trade = true;

   return (trade);
}


